    "@types/adm-zip": "^0.5.5",
    "adm-zip": "^0.5.14",
    "rollup": "^4.18.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-compression": "^1.0.3",
    "rollup-plugin-copy-assets": "^2.0.3",
    "rollup-plugin-zip": "^1.0.3",
    "ts-node": "^10.9.2"
  }
